<?xml version="1.0"?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro" name="firefighter" >
	
<xacro:property name="width" value=".2" />


  <!-- Define the base_link -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0 0 0"/> <!-- Example dimensions; adjust as necessary -->
      </geometry>
      <material name="orange">
        <color rgba="1 0.423 0 1"/> <!-- Example color; adjust as necessary -->
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.05"/> <!-- Match visual geometry for simplicity -->
      </geometry>
    </collision>
  </link>
  
  <joint name="base_to_joint1" type="fixed">
    <parent link="base_link"/>
    <child link="joint1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/> <!-- Example origin; adjust as necessary -->
  </joint>


  <link name="joint1">
    <visual>
      <geometry>
	     <!--- 0.0 0 -0.04  1.5708 3.14159-->
       <mesh filename="package://mycobot/meshes/joint1.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0 " rpy = " 0 0 -1.5708"/>
    </visual>
    <collision>
      <geometry>
	     <!--- 0.0 0 -0.04  1.5708 3.14159-->
       <mesh filename="package://mycobot/meshes/joint1.dae"/>
      </geometry>
        <origin xyz = "0.0 0 0 " rpy = " 0 0 -1.5708"/>
    </collision>
  </link>
  
  <joint name="joint2_to_joint1" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint1"/>
    <child link="joint2"/>
    <origin xyz= "0 0 0.13156" rpy = "0 0 0"/>  
  </joint>

  <link name="joint2">
    <visual>
      <geometry>
       <mesh filename="package://mycobot/meshes/joint2.dae"/>
      </geometry>
      <origin xyz = "0.0 0 -0.06096 " rpy = " 0 0 -1.5708"/>
    </visual>
    <collision>
     <geometry>
       <mesh filename="package://mycobot/meshes/joint2.dae"/>
      </geometry>
      <origin xyz = "0.0 0 -0.06096 " rpy = " 0 0 -1.5708"/>
    </collision>
  </link>


  <joint name="joint3_to_joint2" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint2"/>
    <child link="joint3"/>
    <origin xyz= "0 0  0" rpy = "0 1.5708 -1.5708"/>  
  </joint>

  <link name="joint3">
    <visual>
      <geometry>
       <mesh filename="package://mycobot/meshes/joint3.dae"/>
      </geometry>
    <origin xyz = "0.0 0 0.03256 " rpy = " 0 -1.5708 0"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="package://mycobot/meshes/joint3.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0.03256 " rpy = " 0 -1.5708 0"/>
    </collision>
  </link>

  <joint name="joint4_to_joint3" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint3"/>
    <child link="joint4"/>
    <origin xyz= "  -0.1104 0 0   " rpy = "0 0 0"/>  
  </joint>

  <link name="joint4">
    <visual>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint4.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0.03056 " rpy = " 0 -1.5708 0"/>
    </visual>
    <collision>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint4.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0.03056 " rpy = " 0 -1.5708 0"/>
    </collision>
  </link>
  

  <joint name="joint5_to_joint4" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint4"/>
    <child link="joint5"/>
    <origin xyz= "-0.096 0 0.06462" rpy = "0 0 -1.5708"/>  
  </joint>


  <link name="joint5">
    <visual>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint5.dae"/>
      </geometry>
    <origin xyz = "0.0 0 -0.03356 " rpy = " -1.5708 0 0"/>
    </visual>
    <collision>
       <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint5.dae"/>
      </geometry>
      <origin xyz = "0.0 0 -0.03356 " rpy = " -1.5708 0 0"/>
    </collision>
  </link>


  <link name="joint6">
    <visual>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint6.dae"/>
      </geometry>
    <origin xyz = "0 0.00 -0.038 " rpy = " 0 0 0"/>
    </visual>
    <collision>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint6.dae"/>
      </geometry>
    <origin xyz = "0 0.00 -0.038 " rpy = " 0 0 0"/>
    </collision>
  </link>
  
  
  <joint name="joint6_to_joint5" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint5"/>
    <child link="joint6"/>
    <origin xyz= "0 -0.07318 0" rpy = "1.5708 -1.5708 0"/>  
  </joint>
  
  
  <joint name="joint6output_to_joint6" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="joint6"/>
    <child link="joint6_flange"/>
    <origin xyz= "0 0.0456 0" rpy = "-1.5708 0 0"/>  
  </joint>



  <link name="joint6_flange">
    <visual>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint7.dae"/>
      </geometry>
    <origin xyz = "0.0 0 -0.012 " rpy = " 0 0 0"/>
    </visual>
    <collision>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot/meshes/joint7.dae"/>
      </geometry>
    <origin xyz = "0.0 0 -0.012 " rpy = " 0 0 0"/>
    </collision>
  </link>

  <!-- Gripper attached to end-effector flange (tuned values from gripper_tuner_realtime.py) -->
  <joint name="gripper_to_flange" type="fixed">
    <parent link="joint6_flange"/>
    <child link="gripper"/>
    <origin xyz="0.00 0.00 0.02" rpy="-0.033 0.033 -3.142"/>
  </joint>

  <link name="gripper">
    <visual>
      <geometry>
        <mesh filename="package://mycobot/meshes/Untitled.stl" scale="0.115 0.115 0.115"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="gripper_gray">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://mycobot/meshes/Untitled.stl" scale="0.117 0.117 0.117"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>




















</robot>

